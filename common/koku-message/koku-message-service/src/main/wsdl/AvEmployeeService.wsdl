	</message>
	<portType name='KokuLooraAppointmentService'>
		<operation name='getAppointmentById' parameterOrder='getAppointmentById'>
			<input message='tns:KokuLooraAppointmentService_getAppointmentById'></input>
			<output message='tns:KokuLooraAppointmentService_getAppointmentByIdResponse'></output>
		</operation>
		<operation name='getCreatedAppointments' parameterOrder='getCreatedAppointments'>
