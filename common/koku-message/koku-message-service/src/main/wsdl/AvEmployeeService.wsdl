			<output
				message='tns:KokuLooraAppointmentService_getProcessedAppointmentsResponse'></output>
		</operation>
		<operation name='getTotalCreatedAppointments'
			parameterOrder='getTotalCreatedAppointments'>
			<input message='tns:KokuLooraAppointmentService_getTotalCreatedAppointments'></input>
			<output
				message='tns:KokuLooraAppointmentService_getTotalCreatedAppointmentsResponse'></output>
		</operation>
		<operation name='getTotalProcessedAppointments'
			parameterOrder='getTotalProcessedAppointments'>
			<input
				message='tns:KokuLooraAppointmentService_getTotalProcessedAppointments'></input>
			<output
				message='tns:KokuLooraAppointmentService_getTotalProcessedAppointmentsResponse'></output>
